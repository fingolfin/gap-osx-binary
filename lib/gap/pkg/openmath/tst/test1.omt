<OMOBJ>
  <OMA>
    <OMS cd="basic" name="+"/>
    <OMI>10</OMI>
    <OMI>123912873981723981729387129387912738</OMI>
    <OMI>0</OMI>
    <OMI>-10</OMI>
    <OMI>-123912873981723981729387129387912738</OMI>
    <!-- test for 32 bit int limit -->
    <OMI>-2147483648</OMI>
    <OMI>2147483647</OMI>
    <OMI>-2147483649</OMI>
    <OMI>2147483648</OMI>
    <OMI>-214748364</OMI>
    <OMI>214748364</OMI>
    <OMI>-2147483640</OMI>
    <OMI>2147483640</OMI>
    <!-- test for big ints expressed in base 16 -->
    <OMI>xffffffff</OMI>
    <OMI>-xffffffff</OMI>
    <OMI>x10ab2937fed2837a028374</OMI>
    <OMI>-x10ab2937fed2837a028374</OMI>
    <OMATTR>
      <OMATP>
        <OMS cd="presentation" name="font"/>
        <OMSTR>helvetica</OMSTR>
        <OMS cd="fonts" name="outline"/>
        <OMB>AAECAwQFBgcICQoLDA0=
        </OMB>
      </OMATP>
      <OMV name="y"/>
    </OMATTR>
    <!-- test sharing -->
    <OMA>
      <OMS cd="basic" name="sin"/>
      <OMS cd="basic" name="pi"/>
    </OMA>
    <OMA>
      <OMS cd="basic" name="cos"/>
      <OMS cd="basic" name="pi"/>
    </OMA>
    <OMV name="x"/>
    <!-- test for sharing and small ints (binary) -->
    <OMA>
      <OMS cd="basic" name="+"/>
      <OMI>1</OMI>
      <OMI>2</OMI>
      <OMI>-1</OMI>
      <OMI>-2</OMI>
      <OMA>
        <OMS cd="basic" name="+"/>
        <OMI>126</OMI>
        <OMI>127</OMI>
        <OMI>128</OMI>
        <OMI>129</OMI>
      </OMA>
    </OMA>
  </OMA>
</OMOBJ>
<!-- Another object. test for sharing scope errors -->
<OMOBJ>
  <OMA>
    <OMS cd="basic" name="+"/>
    <OMV name="x"/>
    <OMV name="y"/>
    <OMV name="z"/>
    <OMA>
      <OMS cd="basic" name="+"/>
      <OMV name="x"/>
      <OMV name="y"/>
      <OMV name="z"/>
      <OMV name="z"/>
      <OMV name="zz"/>
      <OMV name="t"/>
      <OMS cd="basic" name="pi"/>
      <!-- this one must not be shared -->
    </OMA>
  </OMA>
</OMOBJ>
<!-- test for binders -->
<OMOBJ>
  <OMBIND>
    <!-- binder -->
    <OMS cd="quant" name="foreach"/>
    <OMBVAR>
      <!-- bound variable(s) -->
      <OMV name="x"/>
    </OMBVAR>
    <!-- bound object -->
    <OMA>
      <OMS cd="basic" name="="/>
      <OMV name="x"/>
      <OMV name="i"/>
    </OMA>
  </OMBIND>
</OMOBJ>
