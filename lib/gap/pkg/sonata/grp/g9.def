#############################################################################
##  File:  GTW9.def                                                          ##
#############################################################################
##  This defines all groups of low order without using any library.
#############################################################################
##
GTW9_1            := Group( (1,2,3,4,5,6,7,8,9) );        
SetName( GTW9_1,  "9/1" );   # cyclic group of order 9
GTW9_2            := Group( (1,2,3) , (4,5,6) );
SetName( GTW9_2,  "9/2" ); 

