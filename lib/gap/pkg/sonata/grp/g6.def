#############################################################################
##  File:  GTW6.def                                                          ##
#############################################################################
##  This defines all groups of low order without using any library.
#############################################################################
##
GTW6_1            := Group( (1,2,3,4,5,6) );        
SetName( GTW6_1,  "6/1" );   # cyclic group order 6 
GTW6_2            := Group( (1,2), (1,2,3) );     
SetName( GTW6_2,  "6/2" );   # S3 
