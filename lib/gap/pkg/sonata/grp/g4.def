#############################################################################
##  File: GTW4.def                                                           ##
#############################################################################
##  This defines all groups of low order without using any library.
#############################################################################
##
GTW4_1            := Group( (1,2,3,4) );        
SetName( GTW4_1,  "4/1" );   # cyclic group order 4 
GTW4_2            := Group( (1,2), (3,4) ); 
SetName( GTW4_2,  "4/2" );   # Klein's four group
