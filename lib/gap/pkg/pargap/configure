fi

if test -n "$ac_init_help"; then
  case $ac_init_help in
     short | recursive ) echo "Configuration of pargap 1.3:";;
   esac
  cat <<\_ACEOF

Optional Packages:
  --with-PACKAGE[=ARG]    use PACKAGE [ARG=yes]
  --without-PACKAGE       do not use PACKAGE (same as --with-PACKAGE=no)
  --with-mpi=system|MPINU2|MPINU
                          Specify the MPI library to use. By default,
                          configure tries to find a working MPI installation
                          on the standard paths. Failing this, it drops back
                          to using the subset MPI implementation included with
