  fi

  # And check the compiler used
  if test "x$MPICC" != "x"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether GAP compiler is set to $MPICC" >&5
$as_echo_n "checking whether GAP compiler is set to $MPICC... " >&6; }

    val=$(expr "$GAPARCH" : ".*-$MPICC")
    if test "x$val" != "x0"; then
