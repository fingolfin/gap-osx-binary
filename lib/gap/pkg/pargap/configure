$as_echo_n "checking for GAP root directory... " >&6; }
  if test -e ${GAPPATH}/${GAPINI}; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: ${GAPPATH}" >&5
$as_echo "${GAPPATH}" >&6; }
    have_gapini=yes
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: Not found" >&5
$as_echo "Not found" >&6; }
    break;
  fi

  # check for sysinfo.gap (to see whether GAP has been built before)
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for GAP sysinfo.gap" >&5
$as_echo_n "checking for GAP sysinfo.gap... " >&6; }
  if test -e ${GAPPATH}/${SYSINFO}; then
    have_sysinfo=yes
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: OK" >&5
$as_echo "OK" >&6; }
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: Not found" >&5
$as_echo "Not found" >&6; }
    break;
  fi

  # check for the source files
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for GAP source files" >&5
$as_echo_n "checking for GAP source files... " >&6; }
  if test -e ${GAPPATH}/${SRC_GAPC}; then
    have_gapsrc=yes
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: OK" >&5
$as_echo "OK" >&6; }
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: Not found" >&5
$as_echo "Not found" >&6; }
    break;
  fi
done

if test "x$have_gapini" = "x"; then
